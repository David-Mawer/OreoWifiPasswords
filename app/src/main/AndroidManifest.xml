<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pithsoftware.wifipasswords">

    <application
        android:name="com.pithsoftware.wifipasswords.extras.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.pithsoftware.wifipasswords.activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pithsoftware.wifipasswords.activities.SettingsActivity"
            android:label="@string/action_settings" />
        <activity
            android:name="com.pithsoftware.wifipasswords.activities.IntroActivity"
            android:label="@string/activity_title_intro" />
        <activity
            android:name="com.pithsoftware.wifipasswords.activities.PasscodeActivity"
            android:label="@string/activity_title_passcode" />
    </application>
</manifest>
